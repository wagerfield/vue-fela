{
  "name": "vue-fela-plugin",
  "version": "0.0.0",
  "description": "Vue Fela Plugin",
  "homepage": "https://github.com/wagerfield/vue-fela#readme",
  "author": "Matthew Wagerfield <matthew@wagerfield.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "fela",
    "plugin"
  ],
  "files": [
    "index.js",
    "provider.js",
    "utils.js"
  ],
  "main": "index.js",
  "jest": {
    "mapCoverage": true,
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "scripts": {
    "precommit": "npm run lint && npm run test",
    "lint": "eslint .",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wagerfield/vue-fela.git"
  },
  "bugs": {
    "url": "https://github.com/wagerfield/vue-fela/issues"
  },
  "dependencies": {
    "fela": "^6.1.0",
    "fela-dom": "^7.0.0"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.12.1",
    "eslint-config-supermind": "^0.47.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.3.0",
    "vue": "^2.5.9",
    "vue-jest": "^1.2.0",
    "vue-server-renderer": "^2.5.10",
    "vue-template-compiler": "^2.5.9",
    "vue-test-utils": "^1.0.0-beta.6"
  }
}
